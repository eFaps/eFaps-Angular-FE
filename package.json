{
  "name": "efaps-angular-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0",
    "start:prod": "ng serve --configuration production --proxy-config proxy.conf.json --host 0.0.0.0",
    "build": "ng build --configuration production",
    "build-demo": "ng build --configuration demo",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers ChromeHeadlessNoSandbox --no-progress --no-watch",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~18.0.1",
    "@angular/common": "~18.0.1",
    "@angular/compiler": "~18.0.1",
    "@angular/core": "~18.0.1",
    "@angular/forms": "~18.0.1",
    "@angular/platform-browser": "~18.0.1",
    "@angular/platform-browser-dynamic": "~18.0.1",
    "@angular/router": "~18.0.1",
    "@efaps/ngx-store": "^8.0.0",
    "angular-gridster2": "^17.0.0",
    "chart.js": "^4.4.3",
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "keycloak-angular": "^15.2.1",
    "keycloak-js": "^22.0.5",
    "primeicons": "^6.0.1",
    "primeng": "^17.17.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "zone.js": "^0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~18.0.2",
    "@angular/cli": "~18.0.2",
    "@angular/compiler-cli": "~18.0.1",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~4.3.0",
    "@types/uuid": "^9.0.8",
    "import-sort-style-module": "^6.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-import-sort": "^0.0.7",
    "typescript": "~5.4.5"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  }
}