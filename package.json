{
  "name": "efaps-angular-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0",
    "start:prod": "ng serve --configuration production --proxy-config proxy.conf.json --host 0.0.0.0",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers ChromeHeadlessNoSandbox --no-progress --no-watch",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~16.0.0",
    "@angular/common": "~16.0.0",
    "@angular/compiler": "~16.0.0",
    "@angular/core": "~16.0.0",
    "@angular/forms": "~16.0.5",
    "@angular/platform-browser": "~16.0.0",
    "@angular/platform-browser-dynamic": "~16.0.0",
    "@angular/router": "~16.0.0",
    "@efaps/ngx-store": "^7.0.0",
    "angular-gridster2": "^16.0.0",
    "chart.js": "^4.3.3",
    "keycloak-angular": "^14.0.0",
    "keycloak-js": "^21.1.2",
    "primeicons": "^6.0.1",
    "primeng": "16.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.1",
    "uuid": "^9.0.0",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.0.0",
    "@angular/cli": "~16.0.1",
    "@angular/compiler-cli": "~16.0.1",
    "@types/jasmine": "~4.3.0",
    "@types/uuid": "^9.0.2",
    "import-sort-style-module": "^6.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-import-sort": "^0.0.7",
    "typescript": "~5.0.2"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  }
}
