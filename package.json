{
  "name": "efaps-angular-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0",
    "start:prod": "ng serve --configuration production --proxy-config proxy.conf.json --host 0.0.0.0",
    "build": "ng build --configuration production",
    "build-demo": "ng build --configuration demo",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers chromiumHeadless --no-progress --no-watch",
    "prettier-html": "prettier --print-width 100 --write \"src/**/*.html\" ",
    "prettier-scss": "prettier --write \"src/**/*.scss\" ",
    "prettier-ts": "prettier --write \"src/**/*.ts\"",
    "prettier": "npm run prettier-html && npm run prettier-scss && npm run prettier-ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.2",
    "@angular/common": "^21.1.1",
    "@angular/compiler": "^21.1.1",
    "@angular/core": "^21.1.1",
    "@angular/forms": "^21.1.1",
    "@angular/platform-browser": "^21.1.1",
    "@angular/platform-browser-dynamic": "^21.1.1",
    "@angular/router": "^21.1.1",
    "@fontsource/roboto": "^5.2.9",
    "@primeuix/styled": "^0.7.4",
    "@primeuix/themes": "^2.0.3",
    "angular-gridster2": "^21.0.1",
    "chart.js": "^4.5.1",
    "file-saver": "^2.0.5",
    "javascript-time-ago": "^2.5.12",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "keycloak-angular": "^21.0.0",
    "keycloak-js": "^26.2.2",
    "ngx-localstorage": "^6.0.0",
    "papaparse": "^5.5.3",
    "primeicons": "^7.0.0",
    "primelocale": "^2.2.3",
    "primeng": "^21.0.4",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@angular/build": "^21.1.1",
    "@angular/cli": "^21.1.1",
    "@angular/compiler-cli": "^21.1.1",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~6.0.0",
    "@types/papaparse": "^5.5.2",
    "@vitest/browser-playwright": "^4.0.18",
    "import-sort-style-module": "^6.0.0",
    "jasmine-core": "~6.0.1",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "~5.9.3",
    "vitest": "^4.0.18"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "bracketSameLine": true
  },
  "overrides": {
    "keycloak-angular": {
      "@angular/common": "21.1.1",
      "@angular/core": "^21.1.1",
      "@angular/router": "^21.1.1",
      "rxjs": "~7.8.2"
    }
  }
}
